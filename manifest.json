{
    "manifest_version": 3,
    "name": "Study Timer",
    "version": "1.0",
    "description": "Blocks entertainment websites while studying.",
    "permissions": ["webRequest", "webRequestBlocking", "declarativeNetRequest"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  